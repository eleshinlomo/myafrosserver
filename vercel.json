{   
    "version": 2,
    "builds": [
      {
        "src": "myafros_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "disDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "staticfiles_build/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "myafros_project/wsgi.py"
      }
    ]
  }